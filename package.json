{
  "name": "@raisaroj/n8n-nodes-text-parser",
  "version": "0.1.53",
  "description": "n8n node to parse and analyze text, extract characters, dialogues, and structure from fiction or large documents.",
  "keywords": [
    "n8n-community-node-package",
    "text parser",
    "text splitting",
    "text chunking",
    "text manipulation"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "",
  "author": {
    "name": "rAi",
    "email": "raisaroj360@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dioveath/n8n-nodes-text-parser.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "pnpm run build && pnpm run lint -c .eslintrc.prepublish.js nodes package.json",
    "deploy": "tsc && gulp build:icons && docker compose up"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/TextParser/TextParser.node.js",
      "dist/nodes/TextAligner/TextAligner.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@types/node": "^24.6.2",
    "expr-eval": "^2.0.2",
    "wink-eng-lite-web-model": "^1.8.1",
    "wink-nlp": "^2.4.0"
  }
}